<script type="text/javascript">
    $("#add").click(function () {
        $("#divnew").show();
        $("#divdetails").hide();
        $("#divedit").hide();
        var variant = $("#Variant").val();
        $("#c_variant").val(variant);
    });

    $(".cancel").click(function () {
        $("#divnew").hide();
        $("#divdetails").show();
        $("#divedit").hide();
    });

    $(".Reset").click(function () {
        $("#Machine").val('0');
        $("#Variant").val('0');
        $("#div_totalcycle").hide();
    });
</script>

<script type="text/javascript">

    $(document).on("keydown", ".space", function (evt) {

        var caretPos = $(this)[0].selectionStart
        if (evt.keyCode == 32 && caretPos == 0) {
            return false;
        }
    });
    var URL = '@System.Configuration.ConfigurationManager.AppSettings["url"]';

    $("body").on("click", "#other_edit", function () {
        var row = $(this).closest("tr");
        var CID = row.find(".CID").val();
        var Variant = $("#Variant").val();
        var Movement = row.find(".Movement").val();
        var Type = row.find(".Type").val();

        var Machine = $("#Machine").val();

        $("#eQueryType").val("Insert_others");

        $("#divedit").show();
        $("#divnew").hide();
        $("#divdetails").hide();

        if (Variant == 'All') {
            $('#eID').val('0');
            $('#div_applicable').show();
        }
        else {
            $('#eID').val(response.data.Table[0].ID);
            $('#div_applicable').hide();
        }

        $('#eMovement').val(Movement);
        $('#eCycle_time').val('0');

        $('.eMachine').val(Machine);

        var s = '<option value="">Please Select</option>';
        if (Type == "Auto") {
            s += '<option value="Auto" selected=selected>Auto</option>';
            s += '<option value="Manual">Manual</option>';
        }
        else {
            s += '<option value="Auto">Auto</option>';
            s += '<option value="Manual" selected=selected>Manual</option>';
        }

        $("#eType").html(s);

    });


    $("body").on("click", "#edit", function () {
        var row = $(this).closest("tr");
        //var variant = $("#ce_variant").val();
        //$("#c_variant").val(variant);
        var CID = row.find(".CID").val();
        var Variant = $("#Variant").val();
        var Machine = $("#Machine").val();
        var Movement = row.find(".Movement").val();
        var Type = row.find(".Type").val();

        $('.eMachine').val(Machine);

        if (Variant == 'All') {
            var myData = {
                "QueryType": "Edit_cycle_time",
                "Parameter": Movement,
                "Parameter1": Type,
                "Parameter2": 'All',
                "Parameter3": Machine,
                "Parameter4": '@Session["CompanyCode"]',
                "Parameter5": '@Session["PlantCode"]',
                "Parameter6": '@Session["LineCode"]',
                "CompanyCode": '@Session["CompanyCode"]',
                    "PlantCode": '@Session["PlantCode"]',
                    "LineCode": '@Session["LineCode"]'
            };
        }
        else {
            var myData = {
                "QueryType": "Edit_cycle_time",
                "Parameter": CID,
                "CompanyCode": '@Session["CompanyCode"]',
                    "PlantCode": '@Session["PlantCode"]',
                    "LineCode": '@Session["LineCode"]'
            };
        }

        $("#eQueryType").val("Update");

        $("#divedit").show();
        $("#divnew").hide();
        $("#divdetails").hide();

        var R_url = '@Url.Action("Login", "Main")';
        var user1 = '@Session["Token"]' + ':' + '@Session["UserName"]';
        $.ajax({
            type: "POST",
            url: URL + 'api/Values/User_settings_details',
            data: myData,
            headers: {
                Authorization: 'Bearer ' + user1
            },
            dataType: "json",
            beforeSend: function () {
                $('.loading').show();
            },
            complete: function () {
                $('.loading').hide();
            },
            success: function (response)
            {
                if (Variant == 'All') {
                    $('#eID').val('0');
                    $('#div_applicable').show();
                }
                else {
                    $('#eID').val(response.data.Table[0].ID);
                    $('#div_applicable').hide();
                }

                $('#eMovement').val(response.data.Table[0].Movement);
                $('#eCycle_time').val(response.data.Table[0].Cycle_time);

                var s = '<option value="">Please Select</option>';
                if (response.data.Table[0].Type == "Auto") {
                    s += '<option value="Auto" selected=selected>Auto</option>';
                    s += '<option value="Manual">Manual</option>';
                }
                else {
                    s += '<option value="Auto">Auto</option>';
                    s += '<option value="Manual" selected=selected>Manual</option>';
                }

                $("#eType").html(s);


            }
            , error: function (response) {
                if (response.status == "401") {
                    swal({
                        icon: "warning",
                        title: "Token is not valid",
                        button: true,
                        closeModal: false
                    })
                    window.location = R_url;
                }
                else {
                    swal({
                        icon: "warning",
                        title: response.responseText,
                        button: true,
                        closeModal: false
                    })

                }
            }
        });
    });

    $("body").on("click", "#delete", function () {
        if (confirm("Do you want to Delete this...?")) {
            var row = $(this).closest("tr");
            var CID = row.find(".CID").val();
            var Movement = row.find(".Movement").val();
            var Type = row.find(".Type").val();
            var Variant = $("#Variant").val();
            var Machine = $("#Machine").val();

            if (Variant == 'All') {
                var myData = {
                    "QueryType": "Delete_allcycletime",
                    "Parameter": Movement,
                    "Parameter1": Type,
                    "Parameter2": 'All',
                    "Parameter3": Machine,
                    "Parameter4": '@Session["CompanyCode"]',
                    "Parameter5": '@Session["PlantCode"]',
                    "Parameter6": '@Session["LineCode"]',
                     "CompanyCode": '@Session["CompanyCode"]',
                    "PlantCode": '@Session["PlantCode"]',
                    "LineCode": '@Session["LineCode"]'
                };
            }
            else {
                var myData = {
                    "QueryType": "Delete_cycletime",
                    "Parameter": CID,
                    "CompanyCode": '@Session["CompanyCode"]',
                    "PlantCode": '@Session["PlantCode"]',
                    "LineCode": '@Session["LineCode"]'
                };
            }

            var R_url = '@Url.Action("Login", "Main")';
            var user1 = '@Session["Token"]' + ':' + '@Session["UserName"]';


            $.ajax({
                type: "POST",
                url: URL + 'api/Values/delete_User_settings_details',
                data: myData,
                headers: {
                    Authorization: 'Bearer ' + user1
                },
                dataType: "json",
                beforeSend: function () {
                    $('.loading').show();
                },
                complete: function () {
                    $('.loading').hide();
                },
                success: function (response) {
                    swal({
                        icon: "success",
                        title: "Details Deleted Successfully...!",
                        button: true,
                        timer: 4500
                    })
                    window.location = '@Url.Action("cycletime_settings", "UserSettings")';
                }
            }).error(function (response) {
                if (response.status == "401") {
                    swal({
                        icon: "warning",
                        title: "Token is not valid",
                        button: true,
                        closeModal: false
                    })
                    window.location = R_url;
                }
                else {
                    swal({
                        icon: "warning",
                        title: response.responseText,
                        button: true,
                        closeModal: false
                    })

                }
            });
        }
    });
</script>

