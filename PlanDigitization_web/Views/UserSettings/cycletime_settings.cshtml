@model IEnumerable<PlanDigitization_web.Models.Cycletime_setting>
@{
    ViewBag.Title = "cycletime_settings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<header class="page-header">
    <h2> Cycle Time </h2>
    <div class="right-wrapper pull-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@Url.Action("MainDashboard","MainDashboard")">
                    <i class="fa fa-home"></i>
                </a>
            </li>
            <li><span>Settings</span></li>
            <li><span>Cycle Time</span></li>
        </ol>
    </div>
</header>


<div class="row">
    <div class="col-md-12">
        <section class="panel">
            <header class="panel-heading">
                <div class="panel-actions">
                </div>
                <h2 class="panel-title">Cycle Time Settings </h2>
            </header>
            <div class="panel-body">

                <div class="form-group">
                    <label class="col-sm-2 control-label"> Machine <span class="required">*</span></label>
                    <div class="col-sm-2" style="right: 100px;">
                        <select id="Machine" class="form-control" onchange="get_variant();">
                            <option value="0">Select</option>
                        </select>
                    </div>
                    <label class="col-sm-2 control-label" style="right: 68px;"> Variant <span class="required">*</span></label>
                    <div class="col-sm-2" style="right: 174px;">
                        <select id="Variant" class="form-control">
                            <option value="0">Select</option>
                        </select>
                    </div>
                    <div class="col-sm-2" style="text-align: center; right: 98px;">
                        <button type="button" id="search_cycle" class="btn btn-primary">Search</button>
                        <button type="button" class="btn btn-danger Reset" style="margin-left: 10px;">Reset</button>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>


<!--Add Cycle Time Details-->
<div class="row" id="divnew" style="display:none;">
    <div class="col-md-12">
        <form class="form" action="@Url.Action("Insert_Update_CycleTime","UserSettings")" method="post">
            <section class="panel">
                <header class="panel-heading" style="background-color:teal;color:white;">
                    <div class="panel-actions">
                    </div>
                    <h2 class="panel-title" style="color:white;">Add New Cycle Time </h2>
                    <input type="hidden" name="QueryType" value="Insert" />
                    <input type="hidden" id="c_variant" name="Variant" value="" />
                    <input type="hidden" name="Machine" value="" class="Mc_id" />

                </header>
                <div class="panel-body">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"> Movement Name <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <input type="text" id="" name="Movement" class="form-control space" placeholder="Movement" required />
                        </div>
                        <label class="col-sm-2 control-label"> Type <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <select name="Type" class="form-control" required>
                                <option value="">Select</option>
                                <option value="Auto">Auto</option>
                                <option value="Manual">Manual</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"> Select if applicable for all variants <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <input type="checkbox" name="if_applicable" value="Y" />
                        </div>

                        <label class="col-sm-2 control-label"> Cycle Time (in secs) <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <input type="text" name="Cycle_time" onkeypress="return isNumberKey(event)" class="form-control space" required />
                        </div>
                    </div>
                </div>
                <footer class="panel-footer">
                    <div class="row">
                        <div class="col-sm-6 col-sm-offset-3" style="text-align:center;">
                            <button type="submit" class="btn btn-primary">Submit</button>
                            <button type="reset" id="cancel" class="btn btn-danger cancel">Cancel</button>
                        </div>
                    </div>
                </footer>
            </section>
        </form>
    </div>
</div>


<!-- Cycle Time Details-->
<div class="row" id="divdetails">
    <div class="col-md-12">
        <section class="panel">
            <header class="panel-heading" style="background-color:teal;color:white;">

                <div class="panel-actions" id="btn_new" style="display:none;">
                    <button class="btn btn-primary" id="add"><i class="fa fa-plus" aria-hidden="true"> Add New Movement </i></button>
                </div>

                @*<button class="btn btn-primary" onclick="exportTableToExcel()"><i class="fa fa-download" aria-hidden="true"> Download </i></button>*@

                <h2 class="panel-title" style="color:white;">List of CycleTime Data </h2>
            </header>
            <div class="panel-body">
                <div class="form-group">
                    <div class="panel-body">
                        <table class="table table-bordered table-striped mb-none" id="datatable_default">
                            <thead style="background-color:teal;color:white;">
                                <tr>
                                    <th>S.no</th>
                                    <th>Movement</th>
                                    <th>Type</th>
                                    <th id="applicable" style="display:block;">If applicable for all variant </th>
                                    <th>Cycle time (in secs)</th>
                                    <th style="text-align:center;width:150px;">Action</th>
                                </tr>
                            </thead>
                            <tbody class="details">
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td>
                                        <button type="button" id="show_total" data-toggle="modal" data-target="#total_model" class="btn btn-primary">Show Total</button>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>


<div class="modal fade" id="total_model" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" style="width:50%;margin-left: 350px;z-index: 999;">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="margin-top:150px;">
            <div class="modal-header" style="padding: 30px 50px !important;border-bottom: 0 !important;">
                <h5 class="modal-title" id="exampleModalLabel">Cycle Time :</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="float: right;">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <section class="panel">
                            <div class="panel-body">
                                <div class="form-group">
                                    <label class="col-sm-6 control-label"> <strong>Auto Cycle Time : </strong> <span id="auto_cycle"> </span> secs </label>
                                    <div class="col-sm-2">

                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-sm-6 control-label"> <strong>Manual Cycle Time : </strong> <span id="manual_cycle"> </span> secs </label>
                                    <div class="col-sm-2">

                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-sm-6 control-label"> <strong>Total Cycle Time : </strong> <span id="total_cycle"> </span> secs </label>
                                    <div class="col-sm-2">

                                    </div>
                                </div>

                            </div>
                        </section>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>


<!--Edit Cycle Time Details-->
<div class="row" id="divedit" style="display:none;">
    <div class="col-md-12">
        <form class="form" action="@Url.Action("Insert_Update_CycleTime","UserSettings")" method="post">
            <section class="panel">
                <header class="panel-heading" style="background-color:teal;color:white;">
                    <div class="panel-actions">
                    </div>
                    <h2 class="panel-title" style="color:white;">Update Cycle Time</h2>
                    <input type="hidden" name="QueryType" id="eQueryType" />
                    <input type="hidden" name="ID" id="eID" />

                    <input type="hidden" id="ce_variant" name="Variant" value="" />
                    <input type="hidden" name="Machine" class="eMachine" />


                    @*<input type="hidden" name="Variant" class="Variant" />*@
                </header>
                <div class="panel-body">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"> Movement Name <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <input type="text" id="eMovement" name="Movement" class="form-control space" readonly placeholder="Movement" required />
                        </div>
                        <label class="col-sm-2 control-label"> Type <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <select name="Type" id="eType" class="form-control" readonly>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <div id="div_applicable">
                            <label class="col-sm-2 control-label"> Select if applicable for all variants <span class="required">*</span></label>
                            <div class="col-sm-4">
                                <input type="checkbox" name="if_applicable" value="Y" checked />
                            </div>
                        </div>
                        <label class="col-sm-2 control-label"> Cycle Time (in secs) <span class="required">*</span></label>
                        <div class="col-sm-4">
                            <input type="text" name="Cycle_time" onkeypress="return isNumberKey(event)" id="eCycle_time" class="form-control space" required />
                        </div>
                    </div>
                </div>
                <footer class="panel-footer">
                    <div class="row">
                        <div class="col-sm-6 col-sm-offset-3" style="text-align:center;">
                            <button type="submit" class="btn btn-primary">Update</button>
                            <button type="reset" id="cancel" class="btn btn-danger cancel">Cancel</button>
                        </div>
                    </div>
                </footer>
            </section>
        </form>
    </div>
</div>


@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/jqueryval")

<script src="~/assets1/javascripts/swal.js"></script>
<script src="~/assets1/javascripts/swal_alert.js"></script>

@if (@TempData["message"] != null)
{
    <script type="text/javascript">
    $(function () {
        var msg = '@TempData["message"]';
        if (msg == "Inserted") {
            swal({
                icon: "success",
                title: "Details Added Successfully...!",
                button: true,
                timer: 4500
            })
        }
        else if (msg == "Updated") {
            swal({
                icon: "success",
                title: "Details Updated Successfully...!",
                button: true,
                timer: 4500
            })
        }
        else if (msg == "Already") {
            swal({
                icon: "error",
                title: "Details Already Added",
                button: true,
                timer: 4500
            })
        }
        else if (msg == "No Variant") {
            swal({
                icon: "error",
                title: "Variants are Not Available...!",
                button: true,
                timer: 4500
            })
        }
        else {
            swal({
                icon: "error",
                title: "Failed to Add",
                button: true,
                timer: 4500
            })
        }
    });
    </script>
}
