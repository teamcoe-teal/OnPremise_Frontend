                            .attr('y', (g) => yScale(g.OEE))
                            .attr('height', (g) => height - yScale(g.OEE))
                            .attr('width', xScale.bandwidth())
                            .on("mouseout", function () {
                                d3.select(this).style("stroke", "none");
                                tooltip.style("visibility", "hidden");
                            })

                            .on("mousemove", function (d) {
                                tooltip.style("visibility", "visible")
                                    .style("top", (d3.event.pageY - 30) + "px").style("left", (d3.event.pageX + 20) + "px");

                                var text = 'A: ' + d.Availability + '<br/>P: ' + d.Performance +  '<br/>Q: ' + d.Quality + '';

                                tooltip.select("div").html(text)

                            })


                            .on('mouseleave', function () {
                                d3.selectAll('.value')
